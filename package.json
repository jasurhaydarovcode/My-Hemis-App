{
  "name": "my-hemis-app",
  "version": "1.0.0",
  "description": "My Hemis App ",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "build": {
    "appId": "com.hemis.app",
    "productName": "HemisApp",
    "directories": {
      "output": "windows"
    },
    "win": {
      "icon": "icons/icon.ico",
      "target": [
        "nsis"
      ]
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jasurhaydarovcode/My-Hemis-App.git"
  },
  "keywords": [
    "HEMIS"
  ],
  "author": "Jasur Haydarov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jasurhaydarovcode/My-Hemis-App/issues"
  },
  "homepage": "https://github.com/jasurhaydarovcode/My-Hemis-App#readme",
  "devDependencies": {
    "electron": "^34.0.2",
    "electron-builder": "^25.1.8"
  }
}